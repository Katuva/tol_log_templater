<Window x:Class="ToL_Log_Templater.Unicode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ToL_Log_Templater"
        mc:Ignorable="d"
        Title="Unicode picker..." Height="347.803" Width="467" ResizeMode="NoResize" ShowInTaskbar="False" Topmost="True" WindowStartupLocation="CenterOwner"
        SizeToContent="WidthAndHeight">
    <Grid Background="#FF2D2D30">
        <UniformGrid x:Name="spContainer" Margin="10,10,10,42" MinWidth="441" MinHeight="267"/>
        <Label x:Name="label" Content="Click to copy to clipboard..." HorizontalAlignment="Left" Margin="10,0,0,10" Foreground="#FF9E9E9E" FontFamily="{DynamicResource Overlock}" FontSize="14" Height="27" VerticalAlignment="Bottom"/>
        <Button x:Name="btnUnicodeEdit" Content="Edit List" Margin="0,0,10,10" Width="75" Height="26" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="btnUnicodeEdit_Click"/>

    </Grid>
</Window>
